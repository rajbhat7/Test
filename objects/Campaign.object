    <fields>
        <fullName>UPC_BUDGETED_vs_ACTUAL_COST__c</fullName>
        <description>Created by Nexell to calculated Δ between Campaign Costs</description>
        <externalId>false</externalId>
        <formula>HierarchyBudgetedCost - HierarchyActualCost</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Δ Budgeted vs Actual Cost</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
