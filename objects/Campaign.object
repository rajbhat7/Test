    <fields>
        <fullName>UPC_Expected_vs_Value_Won__c</fullName>
        <description>Created by Nexell to Calculate the delta between total expected revenue vs value won opportunities.</description>
        <externalId>false</externalId>
        <formula>HierarchyAmountWonOpportunities - HierarchyExpectedRevenue</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Δ Expected vs Value Won</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
