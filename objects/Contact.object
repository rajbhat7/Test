    <validationRules>
        <fullName>NX_Contact_DirectPhone_Format</fullName>
        <active>true</active>
        <description>Used to control phone format.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(UPC_DPhone__c )),
OR( 
    AND( 
        NOT(BEGINS(UPC_DPhone__c ,&quot;+&quot;)), 
        NOT(BEGINS(UPC_DPhone__c ,&quot;0&quot;))), 
        (NOT (REGEX( UPC_DPhone__c , &quot;[&lt;|&gt;]?[0-9,+, ]+&quot;)))
))</errorConditionFormula>
        <errorDisplayField>UPC_DPhone__c</errorDisplayField>
        <errorMessage>Phone numbers must start with + or 0 and must contain only spaces and numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Contact_Fax_Format</fullName>
        <active>true</active>
        <description>Used to control fax format.</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(Fax)),
OR( 
    AND( 
        NOT(BEGINS(Fax,&quot;+&quot;)), 
        NOT(BEGINS(Fax,&quot;0&quot;))), 
        (NOT (REGEX( Fax, &quot;[&lt;|&gt;]?[0-9,+, ]+&quot;)))
))</errorConditionFormula>
        <errorDisplayField>Fax</errorDisplayField>
        <errorMessage>Fax numbers must start with + or 0 and must contain only spaces and numbers.</errorMessage>
    </validationRules>
