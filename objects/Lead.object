    <validationRules>
        <fullName>NX_Emploees_On_Conversion</fullName>
        <active>false</active>
        <description>Number of employees cant be empty on conversion. This is related to credit check that must run on conversion.</description>
        <errorConditionFormula>AND(
IsConverted,
ISBLANK( NumberOfEmployees)
)</errorConditionFormula>
        <errorMessage>Number os Employees is mandatory to convert a Lead.
Please update the Lead and Try Again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Gender_On_Conversion</fullName>
        <active>true</active>
        <description>Gender is mandatory on conversion. This information is mandatory for the credit check.</description>
        <errorConditionFormula>AND(
IsConverted,
ISPICKVAL( UPC_Gender__c , &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>UPC_Gender__c</errorDisplayField>
        <errorMessage>Gender is Mandatory to Convert a Lead.
Please Update the Lead and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Last_Name_On_Conversion</fullName>
        <active>true</active>
        <description>In order to covnert a lead, the contact information must be known. Cant convert an Unknown lead.</description>
        <errorConditionFormula>AND(
IsConverted,
LastName=&quot;Unknown&quot;
)</errorConditionFormula>
        <errorDisplayField>LastName</errorDisplayField>
        <errorMessage>Contact information is mandatory when converting a lead.
Unknown Last Name is not acceptable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Lead_Company_Name_Readonly_Zefix</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(UPC_Zefix_Check_Context__c), 
NOT(ISNULL(UPC_Last_Zefix_Update__c )), 
NOT(UPC_Zefix_Not_Found__c), 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
ISCHANGED( Company ) )</errorConditionFormula>
        <errorDisplayField>Company</errorDisplayField>
        <errorMessage>Company name is locked after it has been checked and found in Zefix</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Lead_Direct_Phone_Format</fullName>
        <active>true</active>
        <description>Used to control phone format.</description>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;,
NOT(ISBLANK(UPC_DPhone__c)), 
OR( 
AND( 
NOT(BEGINS(UPC_DPhone__c,&quot;+&quot;)), 
NOT(BEGINS(UPC_DPhone__c,&quot;0&quot;))), 
(NOT (REGEX( UPC_DPhone__c, &quot;[&lt;|&gt;]?[0-9,+, ]+&quot;))) 
))</errorConditionFormula>
        <errorDisplayField>UPC_DPhone__c</errorDisplayField>
        <errorMessage>Phone numbers must start with + or 0 and must contain only spaces and numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Lead_Fax_Format</fullName>
