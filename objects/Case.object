    <validationRules>
        <fullName>CSCRM_Activation_Date_Today</fullName>
        <active>true</active>
        <description>Activation Date should not be greater than today</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;Closed&quot;) &amp;&amp; RecordType.DeveloperName == &apos;Taifun_Service_Ready&apos; &amp;&amp; UPC_Activation_Date__c &gt; TODAY()</errorConditionFormula>
        <errorMessage>Activation Date should not be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSCRM_Populate_Activation_Date</fullName>
        <active>true</active>
        <description>Populated Activation Date if case is closed</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;Closed&quot;) &amp;&amp; (RecordType.DeveloperName == &apos;Taifun_Service_Ready&apos; || RecordType.DeveloperName == &apos;BP_Provisioning_SF_ServiceClose&apos;) &amp;&amp; ISNULL(UPC_Activation_Date__c)</errorConditionFormula>
        <errorMessage>Activation Date should be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSCRM_Populate_ExternalId</fullName>
        <active>true</active>
        <description>populate external ids on new customer case on case closure</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;Closed&quot;) &amp;&amp; RecordType.DeveloperName == &apos;New_Customer&apos; &amp;&amp; ( ISBLANK(UPC_External_ID__c) || ISBLANK(UPC_Customer_External_ID__c)  )</errorConditionFormula>
        <errorMessage>Please enter 	
External ID and Customer External ID before closing the case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSCRM_Populate_MlogRequestId</fullName>
        <active>true</active>
        <description>populate mlog request id on top net case on case closure</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;Closed&quot;)  &amp;&amp; RecordType.DeveloperName == &apos;Topnet_Order&apos; &amp;&amp; ISBLANK(UPC_mLog_Request_ID__c)</errorConditionFormula>
        <errorMessage>Please enter mLog Request ID before closing the case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSCRM_Populate_TaifunSAP</fullName>
        <active>true</active>
        <description>Validates that Taifun SAP number is populated before case can be closed. This is only applicable for cases with &quot;New Order for a Taifun&quot; record type</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;Closed&quot;) &amp;&amp; (RecordType.DeveloperName == &apos;New_Order_for_a_Taifun&apos; ||  RecordType.DeveloperName == &apos;BDTV_Provisioning_Taifun_ServiceCreation&apos; || RecordType.DeveloperName == &apos;BDTV_MySports_Provisioning_Taifun_ServiceCreation&apos;) &amp;&amp; ISBLANK(UPC_Taifun_SAP__c)</errorConditionFormula>
        <errorMessage>Please enter Taifun SAP number before closing the case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CSCRM_Populate_Termination_Date</fullName>
        <active>true</active>
        <description>Populated termination Date if case is closed</description>
        <errorConditionFormula>ISPICKVAL(Status, &quot;Closed&quot;) &amp;&amp; RecordType.DeveloperName == &apos;Taifun_Termination_Order_Completion&apos; &amp;&amp; ISNULL(UPC_Termination_Date__c)</errorConditionFormula>
        <errorMessage>Termination Date should be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Case_Closing_Without_Account</fullName>
        <active>true</active>
        <errorConditionFormula>not ISPICKVAL(Origin,&quot;internal&quot;) &amp;&amp;  isblank(AccountId) &amp;&amp; ISPICKVAL(Status,&quot;closed&quot;) &amp;&amp;  RecordTypeId = &quot;0121X0000004MfxQAE&quot;</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>The account can&apos;t be empty !</errorMessage>
    </validationRules>
</CustomObject>
