    <fields>
        <fullName>UPC_Case_Age_Business_day__c</fullName>
        <externalId>false</externalId>
        <formula>if (ISPICKVAL ( Status , &quot;Closed&quot;), 
null, 
((5 * ( FLOOR( ( today() - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( today() - DATE( 1900, 1, 8), 7 ) ) ) 
- 
(5 * ( FLOOR( ( datevalue(CreatedDate) - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( datevalue(CreatedDate) - DATE( 1900, 1, 8), 7 ) ) )))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Case Age Business Day</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
