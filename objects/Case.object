        <fullName>UPC_Flag_Case_BCSE__c</fullName>
        <externalId>false</externalId>
        <formula>if (ISPICKVAL ( Status , &quot;Closed&quot;), 
null, 
if ((5 * ( FLOOR( ( today() - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( today() - DATE( 1900, 1, 8), 7 ) ) ) 
- 
(5 * ( FLOOR( ( datevalue(CreatedDate) - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( datevalue(CreatedDate) - DATE( 1900, 1, 8), 7 ) ) ) &lt;= 2,IMAGE(&apos;/resource/flag_green&apos;,&apos;green&apos;,20,20), 
if ((5 * ( FLOOR( ( today() - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( today() - DATE( 1900, 1, 8), 7 ) ) ) 
- 
(5 * ( FLOOR( ( datevalue(CreatedDate) - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( datevalue(CreatedDate) - DATE( 1900, 1, 8), 7 ) ) ) = 3,IMAGE(&apos;/resource/flag_yellow&apos;,&apos;yellow&apos;,20,20), 
if ((5 * ( FLOOR( ( today() - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( today() - DATE( 1900, 1, 8), 7 ) ) ) 
- 
(5 * ( FLOOR( ( datevalue(CreatedDate) - DATE( 1900, 1, 8) ) / 7 ) ) + MIN( 5, MOD( datevalue(CreatedDate) - DATE( 1900, 1, 8), 7 ) ) ) &gt;= 4,IMAGE(&apos;/resource/flag_red&apos;,&apos;red&apos;,20,20), 
NULL))))</formula>
