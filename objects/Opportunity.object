    <validationRules>
        <fullName>NX_B2C_Clarify_ID_Format</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(UPC_B2C_Clarify_ID__c)),
!REGEX( UPC_B2C_Clarify_ID__c ,&quot;^[0-9]{7}-[0-9]{1}$&quot;)
)</errorConditionFormula>
        <errorDisplayField>UPC_B2C_Clarify_ID__c</errorDisplayField>
        <errorMessage>B2C_Clarify_ID must be inserted in this format: 3874939-2</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Lost_Reason_Required_on_oppty_Lost</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IsClosed = TRUE,
IsWon = FALSE,
ISBLANK(TEXT(UPC_Loss_Reason__c)))</errorConditionFormula>
        <errorMessage>When the stage is &quot;Closed Lost&quot;, please add a Loss reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Opportunity_Evolution_Retain_Email</fullName>
        <active>true</active>
        <description>IF &quot;Retain E-Mail&quot; is checked, than  field B2C Customer E-Mail becomes mandatory.</description>
        <errorConditionFormula>AND(
 UPC_B2C_Retain_Email__c,
ISBLANK( UPC_B2C_Customer_E_Mail__c )
)</errorConditionFormula>
        <errorMessage>Retain E-Mail is checked than B2C Customer E-Mail can&apos;t be blank. Please uncheck &quot;Retain E-Mail&quot; or insert a valid value in B2C Customer E-Mail.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Opportunity_Frozen</fullName>
        <active>true</active>
        <description>If Record Type is (frozen): Opp. Name, Close Date, Stage, Currency is Read Only</description>
        <errorConditionFormula>AND( 
OR(RecordType.Name=&quot;New Business (Frozen)&quot;, 
RecordType.Name=&quot;Evolution (Frozen)&quot;, 
RecordType.Name=&quot;Renewal (Frozen)&quot;, 
RecordType.Name=&quot;Termination (Frozen)&quot;), 

OR(ISCHANGED(Name), 
ISCHANGED(CloseDate), 
ISCHANGED(StageName), 
ISCHANGED(CurrencyIsoCode), 
ISCHANGED(ForecastCategoryName) , 
ISCHANGED(OwnerId) ) 

)</errorConditionFormula>
        <errorMessage>These Fields are Read Only, Please refrain from Editing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Opportunity_OrderValidated_no_Reason</fullName>
        <active>true</active>
        <description>When Order is changed to Validated, rejection reason must be empty.</description>
        <errorConditionFormula>OR(

    AND( UPC_Order_Validated__c,
NOT(ISPICKVAL(UPC_Rejection_Reason__c, &quot;&quot;))),

AND( UPC_Order_Validated__c,
 NOT(ISBLANK(UPC_Rejection_Comments__c )))
)</errorConditionFormula>
        <errorMessage>Please clean Rejection Reason and Rejection Comments before approving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Opportunity_Reason_when_not_validated</fullName>
        <active>true</active>
        <description>Created by Nexell, When order not validated is selected, a reason mus be given.</description>
        <errorConditionFormula>AND(ISCHANGED(UPC_Order_Not_Validated_Create_Task__c),
    (UPC_Order_Not_Validated_Create_Task__c),
    ISPICKVAL(UPC_Rejection_Reason__c, &quot;&quot;))</errorConditionFormula>
        <errorMessage>Please select a rejection reason in case it is not validated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Opportunity_Validate_before_Closing</fullName>
        <active>true</active>
        <description>Created by Nexell. This Validation prevents users to close opportunities that are not validated.</description>
        <errorConditionFormula>AND( 
ISCHANGED(StageName), 
IsClosed = TRUE, 
IsWon = TRUE, 
$RecordType.Name &lt;&gt; &quot;Termination&quot;, 
$RecordType.Name &lt;&gt; &quot;Termination (Frozen)&quot;, 
NOT(UPC_Order_Validated__c) )</errorConditionFormula>
        <errorMessage>Before closing, make sure that the order is validated by the back office on closure stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Opportunity_Validated_Not_Validated</fullName>
        <active>true</active>
        <description>Opportunity cant be validated and not validated at the same time.</description>
        <errorConditionFormula>AND(
 UPC_Order_Validated__c ,
 UPC_Order_Not_Validated_Create_Task__c )</errorConditionFormula>
        <errorMessage>Order can not be Validated and Not Validated at the same time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Order_Validation_just_Closure_Stage</fullName>
        <active>true</active>
        <description>An Order can be validated or not validated just on Closure Stage</description>
        <errorConditionFormula>AND(
    OR(
       AND( 
            ISCHANGED(UPC_Order_Not_Validated_Create_Task__c),
            UPC_Order_Not_Validated_Create_Task__c
           ),
       AND(
           ISCHANGED( UPC_Order_Validated__c ),
           UPC_Order_Validated__c
           )
       ),

 NOT(ISPICKVAL(StageName,&apos;Closure&apos;))
)</errorConditionFormula>
        <errorMessage>Order Validation can be done just on Closure Stage. Please move the Opportunity to Closure   to be able to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Req_Synced_Approved_for_Closure</fullName>
        <active>true</active>
        <description>SFCH-2005
The synced product basket is in approval state &quot;no approval needed&quot; or &quot;approved&quot;
If not show error message: &quot;Please submit product basket for approval&quot;</description>
        <errorConditionFormula>AND( 
NOT(UPC_Synchronised_Product_Basket__r.Approval_Status__c=&quot;Approved&quot;), 
NOT(UPC_Synchronised_Product_Basket__r.Approval_Status__c=&quot;No approval needed&quot;), 
NOT(ISBLANK(UPC_Synchronised_Product_Basket__r.Approval_Status__c)), 
ISCHANGED(StageName), 
ISPICKVAL( StageName , &quot;Closure&quot;) 
)</errorConditionFormula>
        <errorMessage>Please submit product basket for approval or wait for approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Req_Synced_Basket_for_Closure</fullName>
        <active>true</active>
        <description>SFCH-2005
A product basket is synced with the opportunity
If not following error message occures: &quot;Please sync a product basket with the opportunity&quot;</description>
        <errorConditionFormula>AND(
ISBLANK(UPC_Synchronised_Product_Basket__c),
ISCHANGED(StageName),
 ISPICKVAL( StageName , &quot;Closure&quot;)
)</errorConditionFormula>
        <errorMessage>Please sync a product basket with the opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Stage_Changing_Receipt_mandatory</fullName>
        <active>true</active>
        <description>make the receipt mandatory is the prior value of stage is entry</description>
        <errorConditionFormula>and(
ischanged( StageName ),
ispickval(priorvalue(StageName),&apos;Entry&apos;),
ISblank(UPC_Receipt_of_Termination__c )
)</errorConditionFormula>
        <errorMessage>Please add a Receipt of Termination before completing Entry stage</errorMessage>
    </validationRules>
