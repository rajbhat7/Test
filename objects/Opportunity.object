        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)} 

function getRecentBasketId() {
    var recentBasketId = &quot;{!Opportunity.Id}&quot;;
    sforce.connection.query(
        &quot;SELECT Name, id, CreatedDate FROM cscfga__Product_Basket__c where cscfga__opportunity__c =&apos;&quot; + &quot;{!Opportunity.Id}&quot; + &quot;&apos;&quot; + &quot; ORDER BY CreatedDate DESC LIMIT 1&quot;,
        {
            onSuccess:
                function (result) {
                    recentBasketId = result.records.Id;
                },
            onFailure:
                function () {
                    console.warn(&apos;### WARNING ### - Creating Quote Without any Product Basket&apos;);
                }
        }
    );
    return recentBasketId;
}

try { 
    var documentId = sforce.apex.execute(&quot;csdg.ConvertController&quot;,&quot;convertToPDF&quot;, 
    {docConfigId:&quot;a2s1q00000005mI&quot;,objectId:getRecentBasketId()}); 

    window.open(&apos;../servlet/servlet.FileDownload?file=&apos;+documentId[0]); 
} catch(e) { 
    alert(&quot;An error occured while creating the document!\n&quot; + e.faultstring); 
}</url>
