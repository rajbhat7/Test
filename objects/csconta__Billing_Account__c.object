        <label>Billing Account Sequence</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Billing Accounts</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>NX_Address_Mandatory_when_Channel_Paper</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

INCLUDES( UPC_Billing_Channel__c ,&quot;Paper Bill&quot;),
ISBLANK(  UPC_Shipping_Address__c  ))</errorConditionFormula>
        <errorDisplayField>UPC_Shipping_Address__c</errorDisplayField>
        <errorMessage>When the Channel is Paper, the Shipping Address field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NX_Email_Mandatory_when_Channel_is_email</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
INCLUDES( UPC_Billing_Channel__c ,&quot;eMail Bill&quot;),
 ISBLANK( UPC_Billing_Email__c ))</errorConditionFormula>
        <errorDisplayField>UPC_Billing_Email__c</errorDisplayField>
        <errorMessage>When the Channel is email, the Billing email field is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>csconta__Override_Account_Billing_Address</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(csconta__Override_Account_Address__c),  NOT(ISBLANK(csconta__Address__c)))</errorConditionFormula>
        <errorDisplayField>csconta__Address__c</errorDisplayField>
        <errorMessage>The address lookup must be cleared if &quot;Override Account Address&quot; is unchecked</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
